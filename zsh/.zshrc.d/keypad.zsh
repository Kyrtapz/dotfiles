# Keypad
# 0 . Enter
bindkey -s "[2~" "0"
bindkey -s "[3~" "."
bindkey -s "OM" "^M"
# 1 2 3
bindkey -s "OF" "1"
bindkey -s "OB" "2"
bindkey -s "[6~" "3"
# 4 5 6
bindkey -s "OD" "4"
bindkey -s "OE" "5"
bindkey -s "OC" "6"
# 7 8 9
bindkey -s "OH" "7"
bindkey -s "OA" "8"
bindkey -s "[5~" "9"
# + -  * /
bindkey -s "Ok" "+"
bindkey -s "Om" "-"
bindkey -s "Oj" "*"
bindkey -s "Oo" "/"
